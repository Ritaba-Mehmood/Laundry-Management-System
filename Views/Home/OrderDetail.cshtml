 @model aptechvision.Models.OrderViewModel

<h2>Order Details</h2>

<div>
    <h4>Order @Model.Id</h4>
    <div>
        <label>Name:</label>
        <p>@Model.Name</p>
    </div>
    <div>
        <label>Phone:</label>
        <p>@Model.Phone</p>
    </div>
    <div>
        <label>Pickup Date:</label>
        <p>@Model.PickupDate</p>
    </div>
    <div>
        <label>Slot:</label>
        <p>@Model.Slot</p>
    </div>
    <div>
        <label>Email:</label>
        <p>@Model.Email</p>
    </div>
    <div>
        <label>Address:</label>
        <p>@Model.Address</p>
    </div>
</div>
